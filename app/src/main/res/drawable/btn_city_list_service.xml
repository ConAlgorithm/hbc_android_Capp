<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="#ffe5e5e5"></solid>
            <corners android:radius="25dp" />
            <stroke android:color="#ffcccccc" android:dashWidth="@dimen/default_line_height" android:width="@dimen/default_line_height"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ffe5e5e5"></solid>
            <corners android:radius="25dp" />
            <stroke android:color="#ffcccccc" android:dashWidth="@dimen/default_line_height" android:width="@dimen/default_line_height"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#fff7f7f7"></solid>
            <corners android:radius="25dp" />
            <stroke android:color="#ffcccccc" android:dashWidth="@dimen/default_line_height" android:width="@dimen/default_line_height"/>
        </shape>
    </item>

</selector>