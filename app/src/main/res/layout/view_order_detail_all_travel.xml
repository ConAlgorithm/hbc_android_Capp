<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/order_detail_travel_parrent_layout">

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="@dimen/order_padding_left"
        android:layout_marginRight="@dimen/order_padding_left"
        android:background="@drawable/shape_order_no_dash"
        android:layerType="software" />

    <TextView
        android:id="@+id/order_detail_all_travel_title_tv"
        style="@style/SkuOrderSubtitleTextView"
        android:paddingLeft="@dimen/order_padding_left"
        android:paddingRight="@dimen/order_padding_left"
        android:text="行程安排" />

    <LinearLayout
        android:id="@+id/order_detail_all_travel_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="12dp"
        android:orientation="vertical">
    </LinearLayout>
</merge>