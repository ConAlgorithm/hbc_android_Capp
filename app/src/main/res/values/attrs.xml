<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="JazzyViewPager">
        <attr name="style_type">
            <enum name="standard" value="0" />
            <enum name="tablet" value="1" />
            <enum name="cubein" value="2" />
            <enum name="cubeout" value="3" />
            <enum name="flipvertical" value="4" />
            <enum name="fliphorizontal" value="5" />
            <enum name="stack" value="6" />
            <enum name="zoomin" value="7" />
            <enum name="zoomout" value="8" />
            <enum name="rotateup" value="9" />
            <enum name="rotatedown" value="10" />
            <enum name="accordion" value="11" />
        </attr>
        <attr name="fadeEnabled" format="boolean" />
        <attr name="outlineEnabled" format="boolean" />
        <attr name="outlineColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RatingView">
        <attr name="rating_bg" format="reference" />
        <attr name="numLevels" format="integer" />
        <attr name="level" format="integer" />
        <attr name="touchable" format="boolean" />
        <attr name="gap" format="dimension"/>
        <attr name="half_bg" format="reference"/>
        <attr name="item_width" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TagGroup">
        <attr name="horizontalSpacing" format="dimension"/>
        <attr name="verticalSpacing" format="dimension"/>
        <attr name="isCenter" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="fhorizontalSpacing" format="dimension" />
        <attr name="fverticalSpacing" format="dimension" />
        <attr name="singleLine" format="boolean" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="debugDraw" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_newLine" format="boolean" />
        <attr name="layout_horizontalSpacing" format="dimension" />
        <attr name="layout_verticalSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LinearGradientImageView">
        <attr name="gradient_orientation" format="enum">
            <enum name="left" value="0x001" />
            <enum name="bottom" value="0x002" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SimpleRatingBar">
        <attr name="srb_borderColor" format="color" />
        <attr name="srb_fillColor" format="color" />
        <attr name="srb_backgroundColor" format="color" />
        <attr name="srb_starBackgroundColor" format="color" />
        <attr name="srb_pressedBorderColor" format="color" />
        <attr name="srb_pressedFillColor" format="color" />
        <attr name="srb_pressedBackgroundColor" format="color" />
        <attr name="srb_pressedStarBackgroundColor" format="color" />
        <attr name="srb_numberOfStars" format="integer" />
        <attr name="srb_starSize" format="dimension" />
        <attr name="srb_maxStarSize" format="dimension" />
        <attr name="srb_starsSeparation" format="dimension" />
        <attr name="srb_starBorderWidth" format="float" />
        <attr name="srb_starCornerRadius" format="float" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_stepSize" format="float" />
        <attr name="srb_drawBorderEnabled" format="boolean" />
        <attr name="srb_gravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="title_res" format="reference"/>
        <attr name="right_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomAttributes">
        <!-- indicate if the slider must show number indicator -->
        <attr name="showNumberIndicator" format="boolean" />
        <!-- in progress view indicate max value of progress -->
        <attr name="max" format="integer" />
        <!-- in progress view indicate min value of progress -->
        <attr name="min" format="integer" />
        <!-- in progress view indicate value of progress -->
        <attr name="value" format="integer" />
    </declare-styleable>

    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="dashOrientation" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CircularProgress">
        <attr name="borderWidth" format="dimension" />
        <attr name="color1" format="color" />
        <attr name="color2" format="color" />
        <attr name="color3" format="color" />
        <attr name="color4" format="color" />
    </declare-styleable>

    <declare-styleable name="OrderInfoItemView">
        <attr name="titleText" format="string"/>
        <attr name="hintText" format="string"/>
        <attr name="isShowBottomLine" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EffectiveShapeView">
        <attr name="shape" format="integer">
            <enum name="circle" value="1"/>
            <enum name="rectangle" value="2"/>
            <enum name="round_rectangle" value="3"/>
            <enum name="square" value="4"/>
            <enum name="polygon" value="5"/>
        </attr>
        <attr name="decorations_direction" format="integer">
            <enum name="left_top" value="1"/>
            <enum name="left_bottom" value="2"/>
            <enum name="right_top" value="3"/>
            <enum name="right_bottom" value="4"/>
        </attr>

        <attr name="decorations_src" format="reference"/>
        <attr name="radius_x" format="dimension"/>
        <attr name="radius_y" format="dimension"/>
        <attr name="sides" format="integer"/>
        <attr name="border_color" format="color" />
        <attr name="border_width" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MultipleTextViewGroup">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textWordMargin" format="dimension" />
        <attr name="textLineMargin" format="dimension" />
        <attr name="textBackground" format="reference" />

        <attr name="textPaddingLeft" format="dimension" />
        <attr name="textPaddingRight" format="dimension" />
        <attr name="textPaddingTop" format="dimension" />
        <attr name="textPaddingBottom" format="dimension" />

        <attr name="overspread" format="boolean" />
        <attr name="columnNum" format="integer" />
        <attr name="onlyOneLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ItemMenu">
        <attr name="itemTitle" format="reference"/>
        <attr name="isHideLine" format="boolean"/>
        <attr name="isHideArrow" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TabView">
        <attr name="text" format="string" />
        <attr name="selected" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
        <attr name="rvp_inertia" format="boolean"/>
        <attr name="rvp_millisecondsPerInch" format="float" />
    </declare-styleable>
</resources>